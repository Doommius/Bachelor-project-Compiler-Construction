# Typecheck and code generation, returning records from functions
type recordType = record of {
                    x: int,
                    y: bool
                  };

func a(x : int, y : bool) : recordType
  var p2 : recordType;
  allocate p2;
  p2.x = x;
  p2.y = y;
  return p2;
end a

var p1 : recordType;
var b : int;
var c : bool;

if (p1.y || c) then write p1.x + b;
